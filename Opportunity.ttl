sf:isWon a owl:DatatypeProperty ;
    rdfs:label "Won" ;
    rdfs:domain sf:Opportunity ;
    rdfs:range rdfs:Literal .

sf:lastModifiedDate a owl:DatatypeProperty ;
    rdfs:label "Last Modified Date" ;
    rdfs:domain sf:Opportunity ;
