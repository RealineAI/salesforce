@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sf: <https://github.com/vocol/salesforce/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

sf:amount a owl:DatatypeProperty ;
    rdfs:label "Amount" ;
    rdfs:domain sf:Opportunity ;
    rdfs:range rdfs:Literal .

sf:campaignId a owl:DatatypeProperty,
        owl:ObjectProperty ;
    rdfs:label "Campaign ID",
        "Campaign ID"^^xsd:string ;
    rdfs:domain sf:Opportunity ;
    rdfs:range rdfs:Literal,
        sf:Campaign .

sf:closeDate a owl:DatatypeProperty ;
    rdfs:label "Close Date" ;
    rdfs:domain sf:Opportunity ;
    rdfs:range rdfs:Literal .

sf:createdById a owl:DatatypeProperty,
        owl:ObjectProperty ;
    rdfs:label "Created By ID",
        "Created By ID"^^xsd:string ;
    rdfs:domain sf:Opportunity ;
    rdfs:range rdfs:Literal,
        sf:User .

sf:createdDate a owl:DatatypeProperty ;
    rdfs:label "Created Date" ;
    rdfs:domain sf:Opportunity ;
    rdfs:range rdfs:Literal .

sf:description a owl:DatatypeProperty ;
    rdfs:label "Description"^^xsd:string ;
    rdfs:domain sf:Opportunity ;
    rdfs:range rdfs:Literal .

sf:expectedRevenue a owl:DatatypeProperty ;
    rdfs:label "Expected Amount" ;
    rdfs:domain sf:Opportunity ;
    rdfs:range rdfs:Literal .

sf:fiscal a owl:DatatypeProperty ;
